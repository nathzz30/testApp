    "@types/sequelize": "^4.28.20",
    "@types/swagger-jsdoc": "^6",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/validator": "^13.12.3",
    "nodemon": "^3.1.9",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
