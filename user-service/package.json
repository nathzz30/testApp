{
  "name": "user-service",
  "version": "1.0.0",
  "description": "User service with PostgreSQL",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node server.ts",
    "dev": "nodemon --ext ts --exec ts-node src/server.ts",
